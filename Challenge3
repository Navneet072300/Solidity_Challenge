pragma solidity ^0.8.0;

contract Calculator{
    function addnum(uint num1, uint num2) public pure returns(uint) {
        return num1+num2;
    }
    function subnum(uint num1, uint num2) public pure returns(uint) {
        return num1-num2;
    }
    function multiplynum(uint num1, uint num2) public pure returns(uint) {
        return num1*num2;
    }
    function dividenum(uint num1, uint num2) public pure returns(uint) {
        return num1/num2;
    }
}
